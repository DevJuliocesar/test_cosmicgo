<!-------- ImplementaciÃ³n de Socket Client ----------->
<script src="/socket.io/socket.io.js"></script>
<script>
    var socket = io.connect('http://localhost:5000');
    socket.on('status', function (data) {
        console.log(`Objeto recibido desde socket.io ${data}`);
        alerta(data);
    });

    /** Mensaje a mostrar **/
    function alerta(data) {
        const key = Object.keys(data)[0];
        return Swal.fire({
            title: 'Insertado con exito!',
            text: `Se ha agregado un nuevo usuario con el ID: ${key} y con el estado ${data[key]}`,
            type: 'success',
            confirmButtonText: 'OK'
        })
    }
</script>